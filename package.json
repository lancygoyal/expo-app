{
  "name": "expo_app",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "expo:ios": "expo start --ios --clear",
    "expo:android": "expo start --android --clear",
    "format:check": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "eslint:check": "eslint --ext .jsx --ext .js src/",
    "eslint:fix": "eslint ./src/**/*.jsx --fix",
    "build:android:preview": "eas build -p android --profile preview",
    "build:ios:preview": "eas build -p ios --profile preview",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=1901' && expo start",
    "test": "jest",
    "testChanged": "jest --watch --coverage=false --changedSince=origin/master",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.12",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react": "~18.0.27",
    "axios": "^1.3.4",
    "axios-hooks": "^4.0.0",
    "expo": "~48.0.9",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.7"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
